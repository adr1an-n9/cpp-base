#include <iostream>
using namespace std;

int main() {
    int numbers[] = {1, 2, 3, 4, 5};
    int searched = 3;
    bool found = false;

    for (int i = 0; i < 5; ++i) {
        if (numbers[i] == searched) {
            found = true;
            break;
        }
    }

    if (found) {
        cout << "The element was found in the array." << endl;
    } else {
        cout << "The element not found in array." << endl;
    }

    return 0;
}
