#include <iostream>
#include <algorithm>

using namespace std;

int main() {
    int numbers[] = {5, 2, 8, 1, 6};
    int n = sizeof(numbers) / sizeof(numbers[0]);

    // Sorting the array
    sort(numbers, numbers + n);

    cout << "The sorted array is: ";
    for (int i = 0; i < n; ++i) {
        cout << numbers[i] << " ";
    }
    cout << endl;

    return 0;
}
