#include <iostream>
#include <climits>
using namespace std;

int main() {
    
    int numbers[]= {5, 2, 8, 1, 6};
    int min = INT_MAX;
    int max = INT_MIN;
    int product = 1;
    
    for ( int i = 0; i < 5; ++i ) {
       if (numbers[i] < min) {
           min = numbers[i];
       }
       if (numbers[i] > max) {
           max = numbers[i];
       }
    }
    
    cout << "The smallest element is: " << min << endl;
    cout << "The largest element is: " << max << endl;
    
    return 0;
}
