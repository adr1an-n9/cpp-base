#include <iostream>
using namespace std;
int main()
{
    int n;
    
    cout<<"n= ";
    cin >> n;
    
    int sum = 0;
    
    while ( n != 0 ) {
        
        sum = sum + n % 10;
        n = n / 10;
        
    }
    
    cout << "The sum of the digits of the number n = " << sum;

    return 0;
}
